<div>
  <div class="row">
    <div class="col-lg-12">
        <h1 style="width:100%; text-align: center;">Welcome to the dna sequence Revolution</h1>
    </div>
  </div>
  
  <div style="display: flex; flex-direction: row;">
     <img src="./images/gout-2870583_1920.jpg" style="height:300;width:100%;">         
  </div>    
  
  <div class="container" style="display: flex; flex-direction: row;">
    <div style="width:50%;">
      <p>
      <pre style="font-size: 16px;">
 It's time to dive deep into the journey 
 through life, 
 and create the future we deserve!!.
      </pre>
     </p> 
    </div>
  
    <div style="width:50%;"> 
     <form method="post" action="/send">        
        <label style="font-size: 20px;">Enter some sequence:</label>
        
        <input type="text" id="myDna" name="data" style="width: 300px; height:300px">
         <input type="submit" value="Submit">
     </form>
    </div>
     
  </div>
 </div>
 <div id="table-wrapper"  style="width:50%">
  <div id="table-scroll">  
 <div class="container">
     
      <% if (dna.length) { %>    
      
         <table id="dnaTable">
            
            <thead>
            <th id="location">location</th>
            <th>coding</th>
            <th>template</th>
            <th>rna</th>
            </thead>
                <tbody>
                    <% dna.forEach((seq) => { %>
                      
                    <tr>
                        <td><h4 class="list-group-item-heading"><%= seq.location %></h4></td>
                        <td><h4 class="list-group-item-heading"><%= seq.coding %></h4></td>                     
                        <td><h4 class="list-group-item-heading"><%= seq.template %></h4></td>
                        <td><h4 class="list-group-item-heading"><%= seq.rna %></h4></td> 
                    </tr>
                   
                <% }) %>
                </tbody>     
                
                  </table>                         
                                                                      
         <% } %>
         </div>
         </div>
          
                 
  </div>
  <p><input type="button" id="bt" value="Show Table Data" onclick="showTableData()" /></p>
    
     <form method="post" action="/aminoAcidPrecentage">        
        <input type="text" id="demo" name="data" ></p>
         <input type="submit" value="amino %">
     </form>   
  <canvas id="myChart" width="400" height="400"></canvas>
   <% if (aa.length) { %>    
      <% aa.forEach((amino) => { %>
                      
                    <tr>
                        <td><h4 class="list-group-item-heading"><%= amino.amino_name %></h4></td>
                        <td><h4 class="list-group-item-heading"><%= amino.precentage %>%</h4></td>                      
                    </tr>
                   
                <% }) %>
     
     <% } %>
