<script>
    function showTableData() {
       var table = document.getElementById("dnaTable");
       var mrna='';      
            // table rows
            for(var i = 1; i < table.rows.length; i++)
            {
                // row cells          
                    mrna = mrna + table.rows[i].cells[3].innerText;
       }
       console.log(typeof(mrna));      
       document.getElementById("demo").innerHTML= mrna
      
    }
</script>


